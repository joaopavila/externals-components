  "scripts": {
    "build:dev": "npm run tmp && npm run inliner && tsc --p .tmp/tsconfig.json && rimraf ./.tmp",
    "build:docs": "rimraf documentation && compodoc -p src/tsconfig.json -n \"Santander Portal Library\"",
    "build": "rimraf lib && npm run tmp && npm run inliner && tsc --p .tmp/tsconfig.json && rimraf ./.tmp",
    "build:aot": "rimraf lib && npm run tmp && npm run inliner && \"./node_modules/.bin/ngc\" -p .tmp/tsconfig.json && rimraf ./.tmp",
    "bundle": "npm run build && npm run build:docs",
    "bundle:precommit": "npm run bundle && git add lib documentation",
    "inliner": "ng2-inline -r true -o ./ -b .tmp/components/ \".tmp/components/**/*.component.ts\"",
    "lint": "tslint \"./src/**/*.ts\"",
    "release": "standard-version",
    "start": "watch \"npm run build:dev\" ./src",
    "test": "karma start",
    "tmp": "rimraf ./.tmp && cpx \"src/**/*.*\" .tmp"
  }
}
